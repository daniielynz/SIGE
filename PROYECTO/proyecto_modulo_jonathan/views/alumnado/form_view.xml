<odoo>
    <record id="view_alumnado_form" model="ir.ui.view">
        <field name="name">alumnado.form</field>
        <field name="model">proyecto_modulo_jonathan.alumnado</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <group>
                        <field name="nombre"/>
                        <field name="apellidos"/>
                        <field name="fecha_nacimiento"/>
                        <field name="direccion"/>
                        <field name="codigo_postal"/>
                        <field name="email"/>
                        <field name="ciclo_formativo"/>
                        <field name="coche"/>
                        <field name="otros"/>
                        <field name="nota_media"/>
                        <field name="nota_media_texto"/>
                    </group>
                </sheet>
            </form>
        </field>
    </record>
</odoo>